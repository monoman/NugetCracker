<?xml version="1.0" encoding="utf-8"?>
<Vsix xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Version="1.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2010">
    <Identifier Id="NuPackToolsVsix.Microsoft.67e54e40-0ae3-42c5-a949-fddf5739e7a5">
        <Name>NuGet Package Manager</Name>
        <Author>Outercurve Foundation</Author>
        <Version>1.7.0.0</Version>
        <Description xml:space="preserve">A collection of tools to automate the process of downloading, installing, upgrading, configuring, and removing packages from a VS Project.</Description>
        <Locale>1033</Locale>
        <MoreInfoUrl>http://docs.nuget.org/</MoreInfoUrl>
        <License>LICENSE.txt</License>
        <GettingStartedGuide>http://docs.nuget.org/docs/start-here/overview</GettingStartedGuide>
        <Icon>Resources\Package.ico</Icon>
        <PreviewImage>Resources\PreviewImage.png</PreviewImage>
        <SupportedProducts>
            <VisualStudio Version="10.0">
                <Edition>Pro</Edition>
                <Edition>IntegratedShell</Edition>
            </VisualStudio>
            <VisualStudio Version="11.0">
                <Edition>Pro</Edition>
                <Edition>IntegratedShell</Edition>
            </VisualStudio>
        </SupportedProducts>
        <SupportedFrameworkRuntimeEdition MinVersion="4.0" MaxVersion="4.0" />
        <AllUsers>false</AllUsers>
    </Identifier>
    <References />
    <Content>
        <MefComponent>|TeamFoundationServer|</MefComponent>
        <MefComponent>|PowerShellHostProvider|</MefComponent>
        <MefComponent>|Console|</MefComponent>
        <VsPackage>|%CurrentProject%;PkgdefProjectOutputGroup|</VsPackage>
        <MefComponent>|VisualStudio|</MefComponent>
        <Assembly AssemblyName="|VisualStudio.Interop;AssemblyName|">|VisualStudio.Interop|</Assembly>
    </Content>
</Vsix>
